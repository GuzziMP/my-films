<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
    <id>7988</id>
    <defaultcontrol>10</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <controls>
        <control>
            <type>image</type>
            <id>0</id>
            <posX>500</posX>
            <posY>320</posY>
            <width>400</width>
            <height>200</height>
            <texture>dialog_background.png</texture>
            <animation effect="fade" time="250">WindowOpen</animation>
            <animation effect="fade" time="250">WindowClose</animation>
        </control>
        <control>
            <description>dialog Heading</description>
            <type>fadelabel</type>
            <id>2</id>
            <posX>550</posX>
            <posY>350</posY>
            <label>934</label>
            <font>font13</font>
            <textcolor>ffffffff</textcolor>
            <animation effect="fade" time="250">WindowOpen</animation>
            <animation effect="fade" time="250">WindowClose</animation>
        </control>
<!--
        <control>
            <description>Rating label</description>
            <type>label</type>
            <id>3</id>
            <posX>707</posX>
            <posY>560</posY>
            <label>#myfilms.db.rating.label</label>
            <font>font13</font>
            <textcolor>ffffffff</textcolor>
            <animation effect="fade" time="250">WindowOpen</animation>
            <animation effect="fade" time="250">WindowClose</animation>
        </control>

-->
        <control>
            <description>Rating value</description>
            <type>label</type>
            <id>4</id>
            <posX>600</posX>
            <posY>380</posY>
            <label>#myfilms.rating</label>
            <font>font16</font>
            <colordiffuse>ffffffff</colordiffuse>
            <textcolor>ffff8000</textcolor>
            <animation effect="fade" time="250">WindowOpen</animation>
            <animation effect="fade" time="250">WindowClose</animation>
        </control>
        <control>
            <description>right button</description>
            <type>button</type>
            <id>10</id>
            <posX>800</posX>
            <posY>450</posY>
            <width>28</width>
            <height>29</height>
            <textureFocus>arrow_round_right_focus.png</textureFocus>
            <textureNoFocus>arrow_round_right_nofocus.png</textureNoFocus>
            <label />
            <onleft>11</onleft>
            <onright>12</onright>
            <onup>12</onup>
            <ondown>12</ondown>
            <animation effect="fade" time="250">WindowOpen</animation>
            <animation effect="fade" time="250">WindowClose</animation>
        </control>
        <control>
            <description>left button</description>
            <type>button</type>
            <id>11</id>
            <posX>550</posX>
            <posY>450</posY>
            <width>28</width>
            <height>29</height>
            <textureFocus>arrow_round_left_focus.png</textureFocus>
            <textureNoFocus>arrow_round_left_nofocus.png</textureNoFocus>
            <label />
            <onleft>12</onleft>
            <onright>10</onright>
            <onup>12</onup>
            <ondown>12</ondown>
            <animation effect="fade" time="250">WindowOpen</animation>
            <animation effect="fade" time="250">WindowClose</animation>
        </control>
        <control>
            <description>OK button</description>
            <type>button</type>
            <id>12</id>
            <posX>830</posX>
            <posY>340</posY>
            <textYOff>9</textYOff>
            <width>30</width>
            <height>30</height>
            <textureFocus>dialogue_ok_focus.png</textureFocus>
            <textureNoFocus>dialogue_ok_nofocus.png</textureNoFocus>
            <label>-</label>
            <onleft>11</onleft>
            <onright>10</onright>
            <onup>11</onup>
            <ondown>10</ondown>
            <animation effect="fade" time="250">WindowOpen</animation>
            <animation effect="fade" time="250">WindowClose</animation>
        </control>
        <control>
            <type>imagelist</type>
            <id>100</id>
            <posX>600</posX>
            <posY>460</posY>
            <width>42</width>
            <height>320</height>
            <textureWidth>30</textureWidth>
            <textureHeight>30</textureHeight>
            <subitems>
                <subitem>greystar.png</subitem>
                <subitem>star.png</subitem>
            </subitems>
            <orientation>horizontal</orientation>
            <percentage>#myfilms.rating</percentage>
            <imagesToDraw>10</imagesToDraw>
            <animation effect="fade" time="250">WindowOpen</animation>
            <animation effect="fade" time="250">WindowClose</animation>
        </control>
    </controls>
</window>
