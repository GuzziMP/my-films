<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="MyFilmsData" targetNamespace="http://tempuri.org/MyFilmsData.xsd" xmlns:mstns="http://tempuri.org/MyFilmsData.xsd" xmlns="http://tempuri.org/MyFilmsData.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="MyFilmsData" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="Watched">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="WatchedEvent" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:attribute name="Date" form="unqualified" type="xs:string" />
                  <xs:attribute name="Config" form="unqualified" type="xs:string" />
                  <xs:attribute name="ID" form="unqualified" type="xs:string" />
                  <xs:attribute name="Title" form="unqualified" type="xs:string" />
                  <xs:attribute name="User" form="unqualified" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="MyFilmsSettings">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Users" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="User" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="Name" form="unqualified" type="xs:string" />
                        <xs:attribute name="Alias" form="unqualified" type="xs:string" />
                        <xs:attribute name="TraktName" form="unqualified" type="xs:string" />
                        <xs:attribute name="Age" form="unqualified" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Persons">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Person" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:attribute name="Name" form="unqualified" type="xs:string" />
                  <xs:attribute name="AlternateName" form="unqualified" type="xs:string" />
                  <xs:attribute name="Born" form="unqualified" type="xs:string" />
                  <xs:attribute name="BirthPlace" form="unqualified" type="xs:string" />
                  <xs:attribute name="MiniBiography" form="unqualified" type="xs:string" />
                  <xs:attribute name="Biography" form="unqualified" type="xs:string" />
                  <xs:attribute name="URL" form="unqualified" type="xs:string" />
                  <xs:attribute name="IMDB_Id" form="unqualified" type="xs:string" />
                  <xs:attribute name="TMDB_Id" form="unqualified" type="xs:string" />
                  <xs:attribute name="IsActor" form="unqualified" type="xs:string" />
                  <xs:attribute name="IsProducer" form="unqualified" type="xs:string" />
                  <xs:attribute name="IsDirector" form="unqualified" type="xs:string" />
                  <xs:attribute name="IsWriter" form="unqualified" type="xs:string" />
                  <xs:attribute name="Photos" form="unqualified" type="xs:string" />
                  <xs:attribute name="Picture" form="unqualified" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>