                </control>
                <control>
                        <description>Heading text label</description>
                        <type>label</type>
                        <id>4</id>
                        <posX>640</posX>
                        <posY>112</posY>
                        <label>181</label>
                        <font>font16</font>
                        <textcolor>FFFFFFFF</textcolor>
