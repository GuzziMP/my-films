                        <onleft>3</onleft>
                        <onright>3</onright>
                        <onup>3</onup>
                        <ondown>3</ondown>
                        <animation effect="fade" time="250">WindowOpen</animation>
                        <animation effect="fade" time="250">WindowClose</animation>
                </control>
                <control>
                        <description>options listcontrol</description>
                        <type>listcontrol</type>
                        <id>3</id>
                        <posX>160</posX>
                        <posY>140</posY>
                        <width>950</width>
                        <height>480</height>
                        <spinPosX>960</spinPosX>
                        <spinPosY>587</spinPosY>
                        <textXOff>-10</textXOff>
                        <IconXOff>-10</IconXOff>
                        <IconYOff>3</IconYOff>
                        <spinCanFocus>no</spinCanFocus>
                        <onleft>10</onleft>
                        <onright>11</onright>
                        <animation effect="fade" time="250">WindowOpen</animation>
                        <animation effect="fade" time="250">WindowClose</animation>
                </control>
				<control>
					<description>Okay button</description>
					<type>button</type>
					<id>10</id>
					<label>186</label>
					<width>100</width>
					<textalign>center</textalign>
					<textYOff>0</textYOff>
					<posX>500</posX>
					<posY>580</posY>
					<onleft>3</onleft>
					<onright>11</onright>
					<onup>3</onup>
					<ondown>3</ondown>
