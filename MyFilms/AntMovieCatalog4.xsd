                        <xs:attribute name="IsProducer" type="xs:boolean" />
                        <xs:attribute name="IsDirector" type="xs:boolean" />
                        <xs:attribute name="IsWriter" type="xs:boolean" />
                        <xs:attribute name="Photos" type="xs:string" />
                        <xs:attribute name="Picture" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="MyFilmsSettings" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Users" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="User" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="Name" type="xs:string" />
                        <xs:attribute name="Alias" type="xs:string" />
                        <xs:attribute name="TraktName" type="xs:string" />
