<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="AntMovieCatalog-4" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="Watched">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Watched" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
      <xs:attribute name="Number" type="xs:string" />
      <xs:attribute name="Date" type="xs:dateTime" />
      <xs:attribute name="User" type="xs:string" />
      <xs:attribute name="Config" type="xs:string" />
      <xs:attribute name="MovieNumber" type="xs:string" />
      <xs:attribute name="Title" type="xs:string" />
    </xs:complexType>
  </xs:element>
